{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "editor.trimAutoWhitespace": true,
  "files.trimTrailingWhitespace": true,
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "prettier.trailingComma": "none",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#ff9840",
    "activityBar.background": "#ff9840",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#dfffee",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#ff9840",
    "statusBar.background": "#ff7c0d",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#d96400",
    "statusBarItem.remoteBackground": "#ff7c0d",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#ff7c0d",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#ff7c0d99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#FF7C0D"
}
